<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.podhead"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/podhead_tubes"
        android:label="@string/string_podhead"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.podhead.MainActivity"
            android:label="@string/string_podhead" 
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            </activity>
        <activity
            android:name="com.example.podhead.Choruspresets"
            android:label="@string/string_podhead" >
            <intent-filter>
                <action android:name="android.intent.action.CHORUSPRESETS" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            </activity>            
        <activity
            android:name="com.example.podhead.Compressorpresets"
            android:label="@string/string_podhead" >
            <intent-filter>
                <action android:name="android.intent.action.COMPRESSORPRESETS" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            </activity>            
        <activity
            android:name="com.example.podhead.Delay"
            android:label="@string/string_podhead" >
