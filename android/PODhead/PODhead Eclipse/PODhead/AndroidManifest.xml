                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            </activity>            
        <activity
            android:name="com.example.podhead.Distortion"
            android:label="@string/string_podhead" >
            <intent-filter>
                <action android:name="android.intent.action.DISTORTION" />
                <category android:name="android.intent.category.DEFAULT" />
